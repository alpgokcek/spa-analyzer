    "raf": "3.4.1",
    "raphael": "2.2.7",
    "react": "16.8.6",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-datetime": "2.16.2",
    "react-dom": "16.8.6",
    "react-dropzone": "7.0.1",
    "react-redux": "6.0.0",
    "react-router": "4.3.1",
