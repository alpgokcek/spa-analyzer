{
  "name": "spa-frontend",
  "version": "1.0.0",
  "description": "spa-frontend",
  "private": true,
  "scripts": {
    "test": "test",
    "build-modernizr": "modernizr -c modernizr-config.json -d src/modernizr.js -u",
    "build-themes": "node-sass-chokidar src/styles/themes -o public/themes -w",
    "lint": "react-scripts lint",
    "postinstall-heroku": "npm run build",
    "wp:server": "webpack --env 'development' --config src/config/webpack.server.js",
    "wp:client": "webpack --env 'development' --config src/config/webpack.client.js",
    "gulpStyles": "gulp",
    "webpack:client": "webpack --env 'development' --config src/config/webpack.client.js --watch",
    "webpack:server": "webpack --env 'development' --config src/config/webpack.server.js --watch",
    "webpack:start": "nodemon --watch build --exec node build/server.js",
    "dev": "npm run wp:client && npm run wp:server && npm-run-all -p webpack:*",
    "build": "npm run clean && webpack --env 'production' --config src/config/webpack.client.js && webpack --env 'production' --config src/config/webpack.server.js",
    "start": "node build/server.js",
    "clean": "rimraf build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.5.0",
    "animate.css": "3.7.0",
    "axios": "^0.21.1",
    "bootstrap": "4.5.3",
    "cloud-config-client": "^1.5.2",
    "concat-map": "0.0.1",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "2.5.7",
